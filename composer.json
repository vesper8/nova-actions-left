{
    "name": "saintsystems/nova-actions-left",
    "version":"5.0.0",
    "description": "Laravel Nova actions on the left",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/nova": "^5.0",
        "nova-kit/nova-packages-tool": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SaintSystems\\NovaActionsLeft\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SaintSystems\\NovaActionsLeft\\NovaActionsLeftServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
